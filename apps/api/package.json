{
	"name": "@repo/api",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev -p 3001",
		"build": "tsc && next build",
		"start": "next start",
		"lint": "next lint",
		"prisma": "prisma",
		"prisma:generate": "dotenv -e .env.local -- prisma generate",
		"prisma:push": "dotenv -e .env.local -- prisma db push",
		"prisma:migrate": "dotenv -e .env.local -- prisma migrate dev",
		"zenstack": "dotenv -e .env.local -- zenstack generate",
		"test": "vitest",
		"test:e2e": "cypress run --config-file cypress.config.ts",
		"test:e2e:dev": "cypress open --config-file cypress.config.ts"
	},
	"dependencies": {
		"@prisma/client": "^5",
		"@repo/auth": "workspace:*",
		"@zenstackhq/runtime": "^2",
		"@zenstackhq/server": "^2",
		"next": "14.2.5",
		"react": "^18",
		"react-dom": "^18"
	},
	"devDependencies": {
		"@types/node": "^20",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"eslint": "^8",
		"eslint-config-next": "14.2.5",
		"prisma": "^5",
		"typescript": "^5",
		"vitest": "^2.0.4",
		"zenstack": "^2",
		"zod": "^3",
		"zod-prisma-types": "^3.1.8"
	}
}