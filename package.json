{
  "name": "vercel-foundation",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel ",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0 --ignore-pattern 'packages/ui/dist/**'",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "cypress run",
    "test:e2e:dev": "cypress open",
    "generate-repo-map": "tree-sitter generate && tree-sitter parse .",
    "tree-sitter:init": "tree-sitter init-config",
    "tree-sitter:generate": "tree-sitter generate",
    "tree-sitter:parse": "tree-sitter parse"
  },
  "dependencies": {
    "@nextui-org/react": "^2",
    "react": "^18",
    "react-dom": "^18",
    "tailwindcss": "^3"
  },
  "devDependencies": {
    "@auth0/nextjs-auth0": "^3.5.0",
    "@eslint/js": "^9",
    "@next/eslint-plugin-next": "^14.2.5",
    "@tailwindcss/typography": "^0.5.13",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@types/node-fetch": "^2.6.11",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.0.4",
    "autoprefixer": "^10",
    "cypress": "^13",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "^13.4.9",
    "eslint-config-prettier": "^8",
    "eslint-config-turbo": "^2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^6.2.2",
    "eslint-plugin-vitest-globals": "^1.5.0",
    "husky": "^9",
    "jsdom": "^24.1.1",
    "lint-staged": "^15",
    "node-mocks-http": "^1.15.0",
    "postcss": "^8",
    "prettier": "^3",
    "react": "^18",
    "react-dom": "^18",
    "tailwindcss": "^3",
    "tsconfig": "^7",
    "tsup": "^8",
    "turbo": "^2.0.9",
    "typescript": "5.3.3",
    "vitest": "^2.0.4",
    "tree-sitter-cli": "^0.20.8",
    "tree-sitter": "^0.20.6"
  },
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">=20"
  }
}
